{
  "symbols.hidesExplorerArrows": false,
  "window.titleBarStyle": "native",
  "apc.font.family": "Bitstream Vera Sans Mono",
  // "editor.fontFamily": "JetBrains Mono",
  // "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font ",
  // "editor.fontFamily": "DM Mono",

  // "editor.fontFamily": "CommitMono",
  // "editor.fontFamily": "DejaVu Sans Mono",
  // "editor.fontFamily": "Bitstream Vera Sans Mono",
  // "editor.fontFamily": "Fira Code",
  // "editor.fontFamily": "Hack",
  // "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace",

  "terminal.integrated.fontFamily": "MeslolGl Nerd Font",
  // "editor.fontLigatures": true,
  "terminal.integrated.fontSize": 14,
  // "apc.font.family": "JetBrains Mono",

  "editor.fontSize": 16.1,
  "editor.fontWeight": "normal",
  "editor.lineHeight": 1.4,
  "editor.wordWrap": "on",
  "editor.tabSize": 2,
  // "editor.cursorSmoothCaretAnimation": "on",
  // "editor.cursorBlinking": "smooth",
  "editor.linkedEditing": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "explorer.compactFolders": false,
  // "workbench.activityBar.location": "hidden",
  "editor.minimap.enabled": false,
  // "editor.scrollbar.horizontal": "hidden",
  // "editor.scrollbar.vertical": "hidden",
  "workbench.statusBar.visible": false,
  "apc.electron": {
    "titleBarStyle": "hiddenInset",
    "trafficLightPosition": {
      "x": 10,
      "y": 10
    },
    "frame": false
  },
  "apc.header": {
    "height": 30,
    "fontSize": 12
  },
  "apc.activityBar": {
    "position": "bottom",
    // "hideSettings": "true",
    "size": 32
  },
  "apc.listRow": {
    "height": 21.4,
    "fontSize": 12
  },
  "apc.statusBar": {
    "position": "editor-bottom",
    "height": 20,
    "fontSize": 10
  },
  "apc.stylesheet": {
    ".title-label > h2": "display : none",
    ".editor-actions": "display :none",
    // ".pane-body": "padding : 6px",
    // ".pane-header": "padding : 0 8px",
    // ".nosidebar .inline-tabs-placeholder": "width : 0px",
    ".split-view-view:first-child .pane-header": "display : none !important;",
    ".monaco-list-row": "border-radius : 4px",
    ".monaco-workbench .monaco-list:not(.element-focused):focus:before": "display :none;"
  },
  // "breadcrumbs.enabled": false,
  "window.commandCenter": false, // hides command center at the top of the window. Use cmd+shift+p instead
  "workbench.layoutControl.enabled": false, // hides those little squares on the top right of the window
  "update.mode": "manual",

  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
    "tab.activeBorder": "#fff",
    "terminal.background": "#00000000"
  },
  "prisma.showPrismaDataPlatformNotification": false,
  "debug.javascript.codelens.npmScripts": "never",
  "[php]": {
    "editor.defaultFormatter": "DEVSENSE.phptools-vscode"
  },
  "workbench.startupEditor": "none",
  "[blade]": {
    "editor.defaultFormatter": "shufo.vscode-blade-formatter"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "workbench.iconTheme": "bearded-icons",
  "workbench.settings.applyToAllProfiles": ["workbench.colorCustomizations"],
  "window.confirmBeforeClose": "always",
  "workbench.fontAliasing": "antialiased",
  "editor.scrollbar.vertical": "hidden",
  "workbench.colorTheme": "Noir Owl",
  "window.zoomLevel": 0.75,
  "npm.autoDetect": "off"
}
